{
  "name": "MIT Textbooks",
  "version": "1.0.0",
  "manifest_version": 2,
  "description": "Integrates MIT Textbooks into your daily browsing.",
  "homepage_url": "http://textbooksearch.mit.edu/",
  "permissions": [
    "*://*/*"
  ],
  "omnibox": {
    "keyword": "tb"
  },
  "icons": {
    "16": "icons/16.png",
    "48": "icons/48.png",
    "128": "icons/128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "*://*/*"
      ],
      "exclude_matches": [
        "*://textbooksearch.mit.edu/*",
        "*://tb.mit.edu/*"
      ],
      "js": [
        "inject.js"
      ],
      "run_at": "document_end"
    }
  ],
   "background": {
      "persistent": false,
      "scripts": ["background.js"]
    }
}